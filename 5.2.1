array_size = int(input("Enter the array size: "))
print("Input {} elements in the array:".format(array_size))
array = list(map(int, input().split()))
non_repeated_elements = []
for num in array:
    if array.count(num) == 1:
        non_repeated_elements.append(num)
    elif array.count(num) > 1 and num in non_repeated_elements:
        non_repeated_elements.remove(num)
print("The non-repeated elements found in the array are:")
for element in non_repeated_elements:
    print(element, end=" ")
